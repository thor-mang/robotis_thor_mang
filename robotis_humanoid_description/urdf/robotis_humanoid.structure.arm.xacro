              <mesh filename="package://robotis_humanoid_description/meshes/robotis_${prefix}_arm_grip_link.stl" scale="0.001 0.001 0.001"/>
      	    </geometry>
      	    <material name="grey"/>
    	  </visual>

          <inertial>
